{
    "name": "adduc/domain-tracker",
    "description": "Domain tracker, inspired by LET member alex's project",
    "license": "MIT",
    "authors": [
        {
            "name": "John Long",
            "email": "a@88k.us"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-pdo": "*",
        "novutec/whoisparser": "dev-master",
        "novutec/domainparser": "dev-master",
        "ircmaxell/password-compat": "1.0.3",
        "doctrine/inflector": "v1.0",
        "swiftmailer/swiftmailer": "v5.0.0"
    },
    "autoload": {
        "files": [ "src/bootstrap.php" ],
        "psr-0": { "": "src" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/novutec/WhoisParser"
        },
        {
            "type": "vcs",
            "url": "https://github.com/novutec/DomainParser"
        }
    ]
